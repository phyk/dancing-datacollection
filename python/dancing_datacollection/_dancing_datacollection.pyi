from enum import Enum
from typing import List, Optional, Dict
from datetime import date

class Level(Enum):
    E = ...
    D = ...
    C = ...
    B = ...
    A = ...
    S = ...

class Style(Enum):
    Standard = ...
    Latein = ...

class Dance(Enum):
    SlowWaltz = ...
    Tango = ...
    VienneseWaltz = ...
    SlowFoxtrot = ...
    Quickstep = ...
    Samba = ...
    ChaChaCha = ...
    Rumba = ...
    PasoDoble = ...
    Jive = ...

class AgeGroup(Enum):
    Juv1 = ...
    Juv2 = ...
    Jun1 = ...
    Jun2 = ...
    Youth = ...
    Adult = ...
    Adult2 = ...
    Sen1 = ...
    Sen2 = ...
    Sen3 = ...
    Sen4 = ...
    Sen5 = ...
    Senior = ...

class Judge:
    @property
    def code(self) -> str: ...
    @property
    def name(self) -> str: ...
    @property
    def club(self) -> Optional[str]: ...

class CommitteeMember:
    @property
    def name(self) -> str: ...
    @property
    def club(self) -> Optional[str]: ...

class Officials:
    @property
    def responsible_person(self) -> Optional[CommitteeMember]: ...
    @property
    def assistant(self) -> Optional[CommitteeMember]: ...
    @property
    def judges(self) -> List[Judge]: ...

class IdentityType(Enum):
    Solo = ...
    Couple = ...

class Participant:
    @property
    def identity_type(self) -> IdentityType: ...
    @property
    def name_one(self) -> str: ...
    @property
    def bib_number(self) -> int: ...
    @property
    def name_two(self) -> Optional[str]: ...
    @property
    def affiliation(self) -> Optional[str]: ...
    @property
    def final_rank(self) -> Optional[int]: ...

class WDSFScore:
    @property
    def technical_quality(self) -> float: ...
    @property
    def movement_to_music(self) -> float: ...
    @property
    def partnering_skills(self) -> float: ...
    @property
    def choreography(self) -> float: ...

class Round:
    @property
    def name(self) -> str: ...
    @property
    def marking_crosses(self) -> Optional[Dict[str, Dict[int, Dict[Dance, bool]]]]: ...
    @property
    def dtv_ranks(self) -> Optional[Dict[str, Dict[int, Dict[Dance, int]]]]: ...
    @property
    def wdsf_scores(self) -> Optional[Dict[str, Dict[int, WDSFScore]]]: ...

class Competition:
    @property
    def level(self) -> Level: ...
    @property
    def age_group(self) -> AgeGroup: ...
    @property
    def style(self) -> Style: ...
    @property
    def dances(self) -> List[Dance]: ...
    @property
    def min_dances(self) -> int: ...
    @property
    def officials(self) -> Officials: ...
    @property
    def participants(self) -> List[Participant]: ...
    @property
    def rounds(self) -> List[Round]: ...

class Event:
    @property
    def name(self) -> str: ...
    @property
    def date(self) -> Optional[date]: ...
    @property
    def organizer(self) -> Optional[str]: ...
    @property
    def hosting_club(self) -> Optional[str]: ...
    @property
    def competitions_list(self) -> List[Competition]: ...

def download_sources(config_path: str) -> None: ...
def extract_competitions(data_dir: str) -> Event: ...
def validate_extracted_competitions(event: Event) -> bool: ...
def collect_dancing_data(config_path: str) -> Event: ...
