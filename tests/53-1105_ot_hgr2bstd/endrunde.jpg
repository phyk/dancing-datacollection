/* Standard-Styling für den Kontakt-Button */
#contact-btn {
    background-color: #dd3333;
    color: #ffffff;
    border: none;
    padding: 10px 20px;
    font-size: 16px;
    cursor: pointer;
    transition: background-color 0.3s ease;
}

/* Hover-Styling */
#contact-btn:hover {
    background-color: #000000;
    color: #ffffff;
}

/* Styling für .ot-button > a:nth-child(1) */
.ot-button > a:nth-child(1) {
    background-color: #dd3333;
    color: #ffffff;
    display: inline-block;
    padding: 10px 20px;
    font-size: 16px;
    text-align: center;
    text-decoration: none;
    border: none;
    cursor: pointer;
    transition: background-color 0.3s ease;
}

/* Hover-Styling für .ot-button > a:nth-child(1) */
.ot-button > a:nth-child(1):hover {
    background-color: #000000;
    color: #ffffff;
}

/* Falls alle Buttons betroffen sein sollen */
button {
    background-color: #dd3333;
    color: #ffffff;
    border: none;
    padding: 10px 20px;
    font-size: 16px;
    cursor: pointer;
    transition: background-color 0.3s ease;
}

button:hover {
    background-color: #000000;
    color: #ffffff;
}

/* Standard-Styling für das zweite input-Element in wpcf7-Formularen */
